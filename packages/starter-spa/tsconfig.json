{
  "compilerOptions": {
    "target": "es2020",
    "module": "es2020",
    "moduleResolution": "node",
    "lib": ["es2020", "dom", "dom.iterable"],
    "declaration": true,
    "sourceMap": true,
    "inlineSources": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "outDir": "./dev",
    "importHelpers": true,
    "tsBuildInfoFile": ".tsbuildinfo",
    "incremental": true
  },
  "include": ["src/**/*.ts"],
  "exclude": []
}
