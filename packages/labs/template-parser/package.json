{
  "name": "@lit-labs/template-parser",
  "version": "0.1.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "lit-html template parsing utilities",
  "license": "BSD-3-Clause",
  "author": "Google LLC",
  "homepage": "https://github.com/lit/lit/tree/main/packages/labs/template-parser",
  "repository": {
    "type": "git",
    "url": "https://github.com/lit/lit.git",
    "directory": "packages/labs/template-parser"
  },
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "wireit",
    "build:esm": "wireit",
    "build:cjs": "wireit",
    "test": "wireit"
  },
  "wireit": {
    "build": {
      "dependencies": [
        "build:esm",
        "build:cjs"
      ]
    },
    "build:esm": {
      "dependencies": [
        "../../lit-html:build"
      ],
      "command": "tsc --build --pretty",
      "files": [
        "src/**/*.ts",
        "tsconfig.json",
        "tsconfig.cjs.json"
      ],
      "output": [
        "lib",
        "test",
        "cjs",
        "index.{js,js.map,d.ts,d.ts.map}",
        "tsconfig.tsbuildinfo",
        "tsconfig.cjs.tsbuildinfo"
      ],
      "clean": "if-file-deleted"
    },
    "build:cjs": {
      "dependencies": [
        "../../lit-html:build"
      ],
      "command": "tsc --project tsconfig.cjs.json --pretty && echo '{\"type\": \"commonjs\"}' > cjs/package.json",
      "files": [
        "src/**/*.ts",
        "tsconfig.json",
        "tsconfig.cjs.json"
      ],
      "output": [
        "cjs",
        "tsconfig.cjs.tsbuildinfo"
      ],
      "clean": "if-file-deleted"
    },
    "test": {
      "#comment": "The quotes around the file regex must be double quotes on windows!",
      "command": "uvu test \"_test\\.js$\"",
      "dependencies": [
        "build"
      ],
      "files": [
        "test-files/"
      ],
      "output": []
    }
  },
  "files": [
    "/lib/",
    "/cjs/",
    "/index.{js,js.map,d.ts,d.ts.map}"
  ],
  "exports": {
    ".": {
      "import": "./index.js",
      "xrequire": "./cjs/index.js"
    },
    "./": {
      "import": "./",
      "xrequire": "./cjs/"
    }
  },
  "dependencies": {
    "parse5": "^7.0.0",
    "lit-html": "^2.2.6"
  },
  "devDependencies": {
    "@types/parse5": "^6.0.1"
  }
}
